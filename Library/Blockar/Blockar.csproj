<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>netstandard2.0;net45;net46;net35</TargetFrameworks>
    <RootNamespace>Daramee.Blockar</RootNamespace>
    <PackageId>Daramee.Blockar</PackageId>
    <Version>3.0.0</Version>
    <Authors>Jin Jae-yeon</Authors>
    <Company>DARAM WORLD</Company>
    <Description>Markup Language Parser and Serializer</Description>
    <Copyright>Copyright â“’ 2014-2020 Jin Jae-yeon</Copyright>
    <RepositoryUrl>git@github.com:daramkun/Blockar.git</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <PackageProjectUrl>https://github.com/daramkun/Blockar</PackageProjectUrl>
    <PackageLicenseFile>README.md</PackageLicenseFile>
  </PropertyGroup>

  <ItemGroup>
    <Compile Remove="Samples\**" />
    <EmbeddedResource Remove="Samples\**" />
    <None Remove="Samples\**" />
  </ItemGroup>

  <ItemGroup>
    <None Include="..\README.md">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

  <Import Project="..\Blockar.Shared\Blockar.Shared.projitems" Label="Shared" />

</Project>
